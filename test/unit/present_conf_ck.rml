
(*[O 2]*)
let process present_conf_ck_else act =
  newclock ck in
    signal s1,s2 in
    emit s1
    ||
    present (s1 /\ s2) then act 1 else act 2

(*[O 1]*)
let process present_conf_ck_then act =
  newclock ck in
    signal s1,s2 in
    emit s1
    ||
    emit s2
    ||
    present (s1 /\ s2) then act 1 else act 2

open Rmltest

let test =
  [ (present_conf_ck_else, "present_conf_ck_else", [O 2]);
    (present_conf_ck_then, "present_conf_ck_then", [O 1]) ]
